meta {
  name: Create Group Member
  type: http
  seq: 1
}

post {
  url: {{base_url}}/group-members
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "groupId": "{{group_id}}",
    "userId": "{{user_id}}",
    "role": "MEMBER"
  }
}

settings {
  encodeUrl: true
}
